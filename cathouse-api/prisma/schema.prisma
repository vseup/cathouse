generator client {
  provider     = "prisma-client"
  output       = "../src/generated/prisma"
  moduleFormat = "cjs"
}

datasource db {
  provider = "postgresql"
}

model Cat {
  id        String   @id @default(uuid()) @db.Uuid
  type      CatType
  name      String   @unique
  donor     String?
  donation  Float
  createdAt DateTime @default(now()) @map("created_at")

  @@map("cat")
}

enum CatType {
  CAT01
  CAT02
  CAT03
  CAT04
  CAT05
  CAT06
  CAT07
  CAT08
  CAT09
  CAT10
}
